{
    "extends": [
      "eslint:recommended"
    ],
   "parserOptions": {
     "ecmaVersion": 2017,
     "sourceType": "module"
   },
   "env": {
     "browser": true,
     "node": true,
     "jquery": true,
     "mocha": true,
     "es6": true
   },
   "rules": {
     "quotes": 0,
     "no-console": 1,
     "no-debugger": 1,
     "no-var": 1,
     "semi": [1, "always"],
     "no-trailing-spaces": 0,
     "eol-last": 0,
     "no-underscore-dangle": 0,
     "no-alert": 2,
     "no-lone-blocks": 0,
     "jsx-quotes": 1,
     "no-unused-vars": 1,
     "no-undef": 1,
     "guard-for-in":1,
     "no-constant-condition": 1
   },
   "globals": {
   }
 }